<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <solid
                android:color="#136BFF"/>
            <stroke
                android:width="4dp"
                android:color="#136BFF"/>
        </shape>

    </item>
    <item>
        <shape android:shape="oval">
            <solid
                android:color="#F2F5FB"/>

            <stroke
                android:width="4dp"
                android:color="#F2F5FB"/>

        </shape>
    </item>
</selector>